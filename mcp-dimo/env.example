# DIMO MCP Server - Environment Variables
# Copy this file to .env and fill in your actual values

# =============================================================================
# DIMO AUTHENTICATION (Required for vehicle data access)
# =============================================================================
# Get these credentials from https://console.dimo.org/
DIMO_CLIENT_ID=your_dimo_client_id_here
DIMO_DOMAIN=your_dimo_domain.com
DIMO_PRIVATE_KEY=your_dimo_private_key_here

# =============================================================================
# SERVER CONFIGURATION
# =============================================================================
# Railway will set this automatically
PORT=3001

# =============================================================================
# CORS SETTINGS (Production)
# =============================================================================
# Add your Vercel frontend domain here
ALLOWED_ORIGINS=https://your-app.vercel.app,http://localhost:8080

# =============================================================================
# LOGGING
# =============================================================================
NODE_ENV=production
LOG_LEVEL=info

# =============================================================================
# SECURITY
# =============================================================================
# Optional: Add API key for additional security
API_KEY=your_optional_api_key_here
